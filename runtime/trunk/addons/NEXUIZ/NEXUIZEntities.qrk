      {
        txt = "&"
        hint = "number of seconds before door returns (default 2, -1 = return "
         "immediately)"
      }
      spawnflags: =
      {
        typ = "X1"
        cap = "START_OPEN"
      }
      spawnflags: =
      {
        typ = "X2"
        cap = "DOOR_DONT_LINK"
      }
      spawnflags: =
      {
        typ = "X4"
        cap = "CRUSHER"
      }
      Sep: =
      {
        Typ = "S"
        Txt = ""
      }
      light: =
      {
        txt = "&"
        hint = "constantLight radius of .md3 model included with entity. Has n"
