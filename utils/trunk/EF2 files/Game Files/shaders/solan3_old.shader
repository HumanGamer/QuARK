	surfaceparm nonsolid	
	
	surfaceparm water
	surfaceparm nolightmap
	qer_editorimage textures/liquids/orange3.tga
	qer_trans 0.5

	{
		map textures/liquids/orange3.tga
		//blendFunc GL_ONE_MINUS_SRC_ALPHA GL_SRC_ALPHA
		blendFunc blend
		tcmod scale 1 0.35
		tcmod scroll 0 0.25
		//rgbGen lightingDiffuse
		alphaGen dot 0 0
	}
