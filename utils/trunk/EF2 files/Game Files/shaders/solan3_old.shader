textures/solan3old/techwall
{

	qer_editorimage textures/solan3old/techwall1.tga
	{
		map textures/shaderfx/techwall1fxblue.tga
		blendfunc blend
		tcMod scroll 0.2 0
		depthWrite
		

	}
	{
		map textures/shaderfx/techwall1fxgreen.tga
		blendfunc add
		tcMod turb 1 0.1 0.026 0.26
		tcMod scroll -0.1 0

	}
	{
		map textures/solan3old/techwall1.tga
		blendfunc GL_ONE GL_SRC_ALPHA
		
		
	}
	{
		map $lightmap
		blendfunc gl_dst_color gl_zero
		rgbGen identity
	}
}
