chief_warrant_officer
{
surfaceparm trans
cull none

	{
		map models/char/misc/chief_warrant_officer.tga
		blendfunc GL_SRC_ALPHA GL_ONE_MINUS_SRC_ALPHA
		rgbGen lightingdiffuse
		depthWrite
	}		
}

