//--------------------------------------------------------------------
// Mission Analysis Background 01
//--------------------------------------------------------------------

mission_analysis_bkg_01
{
   nomipmaps
   nopicmip
   cull none
   surfaceparm nolightmap
   {
      clampmap sysimg\hud\mission_analysis\mission_analysis_bkg_01.tga
      Blendfunc Blend
      alphaGen global
      rgbGen global
   }
}
