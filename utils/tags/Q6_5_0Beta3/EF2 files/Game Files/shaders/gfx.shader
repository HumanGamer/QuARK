//================= 2D Shaders

sysimg/2d/default-map-screen
   {
   nopicmip
   nomipmaps
      {
      noDepthTest
      map sysimg/2d/default-map-screen.tga   
      }
   }

sysimg/2d/backtile
   {
   nopicmip
   nomipmaps
      {
      noDepthTest
      map sysimg/2d/backtile.tga      
      }
   }
   
sysimg/2d/mouse_arrow
   {
   nopicmip
   nomipmaps
   cull none
      {
      noDepthTest
      clampmap sysimg/2d/mouse_arrow.tga
      blendFunc GL_SRC_ALPHA GL_ONE_MINUS_SRC_ALPHA
      rgbGen identity
      alphaGen identity
      }
   }

